{
  "name": "@esdora/kit",
  "type": "module",
  "version": "0.1.0",
  "description": "A library of tools commonly used for development",
  "author": "Esdora <https://github.com/esdora-js/esdora>",
  "license": "MIT",
  "funding": "https://github.com/esdora-js/esdora",
  "homepage": "https://github.com/esdora-js/esdora#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esdora-js/esdora.git"
  },
  "bugs": "https://github.com/esdora-js/esdora/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./function": {
      "import": {
        "types": "./dist/types/function.d.mts",
        "default": "./dist/function.mjs"
      },
      "require": {
        "types": "./dist/types/function.d.cts",
        "default": "./dist/function.cjs"
      }
    },
    "./promise": {
      "import": {
        "types": "./dist/types/promise.d.mts",
        "default": "./dist/promise.mjs"
      },
      "require": {
        "types": "./dist/types/promise.d.cts",
        "default": "./dist/promise.cjs"
      }
    },
    "./validate": {
      "import": {
        "types": "./dist/types/validate.d.mts",
        "default": "./dist/validate.mjs"
      },
      "require": {
        "types": "./dist/types/validate.d.cts",
        "default": "./dist/validate.cjs"
      }
    },
    "./web": {
      "import": {
        "types": "./dist/types/web.d.mts",
        "default": "./dist/web.mjs"
      },
      "require": {
        "types": "./dist/types/web.d.cts",
        "default": "./dist/web.cjs"
      }
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/esdora.min.js",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsx scripts/build.ts",
    "dev": "unbuild --stub",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit"
  }
}
