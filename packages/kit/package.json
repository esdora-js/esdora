{
  "name": "@esdora/kit",
  "type": "module",
  "version": "0.3.0",
  "description": "A library of tools commonly used for development",
  "author": "Esdora <https://github.com/esdora-js/esdora>",
  "license": "MIT",
  "funding": "https://github.com/esdora-js/esdora",
  "homepage": "https://github.com/esdora-js/esdora#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esdora-js/esdora.git"
  },
  "bugs": "https://github.com/esdora-js/esdora/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./experimental": {
      "import": {
        "types": "./dist/experimental.d.ts",
        "default": "./dist/experimental.js"
      },
      "require": {
        "types": "./dist/experimental.d.cts",
        "default": "./dist/experimental.cjs"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "browser": "./dist/esdora-kit.min.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit"
  }
}
